            "nullable": true
          },
          "location": {
            "type": "string",
            "nullable": true
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          },
          "creatorId": {
            "type": "string",
            "nullable": true
          },
          "creator": {
            "$ref": "#/components/schemas/ApplicationUser"
          },
          "registrations": {
            "type": "array",
