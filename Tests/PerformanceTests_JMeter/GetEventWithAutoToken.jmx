          <hashTree/>
          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">// Get the token from JSON Extractor
String token = vars.get(&quot;authToken&quot;);

if (token != null &amp;&amp; !token.equals(&quot;NOT_FOUND&quot;)) {
    // Store as JMeter Property (global)
    props.put(&quot;globalJwtToken&quot;, token);
    log.info(&quot;=== JWT Token stored successfully ===&quot;);
    log.info(&quot;Token: &quot; + token.substring(0, Math.min(20, token.length())) + &quot;...&quot;);
} else {
    log.error(&quot;=== FAILED to extract JWT token ===&quot;);
}</stringProp>
